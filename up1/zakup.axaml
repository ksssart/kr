<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="300" d:DesignHeight="400"
        MaxHeight="400"
        MaxWidth="300"
        x:Class="up1.zakup"
        Title="ЗАКУП">
    
    <Grid>
        <Image Source="D:/apps/pic/100.png" Width="300" Height="400"/>
        <StackPanel Orientation="Horizontal">
            <TextBlock Margin = "10 10 0 0"> ТАБЛИЦА Закуп: </TextBlock>
        </StackPanel>
        
        <DataGrid Name="ZakupGrid" x:CompileBindings="False" Margin ="10 0 0 0" Height="300">
            <DataGrid.Columns>
                <DataGridTextColumn Header="id" Binding="{Binding id}"></DataGridTextColumn>
                <DataGridTextColumn Header="object_id" Binding="{Binding object_id}"></DataGridTextColumn>
                <DataGridTextColumn Header="material_id" Binding="{Binding material_id}"></DataGridTextColumn>
            </DataGrid.Columns>
        </DataGrid>
        <StackPanel Orientation="Horizontal">
            <Button Name="Back" Content="  Назад" Background="RosyBrown" Click="back" Width="80" Height="30" Margin = "10 350 0 0"></Button>
        </StackPanel>
    </Grid> 
</Window>
