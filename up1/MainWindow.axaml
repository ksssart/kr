<Window xmlns="https://github.com/avaloniaui"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
        MaxHeight="230"
        MaxWidth="500"
        x:Class="up1.MainWindow"
        Title="ООО 'КИРПИЧ' ">
        <Grid>
            <Image Source="D:/apps/pic/fon2.png" Width="510" Height="230"/>
            <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
              <TextBlock Text="Здравствуйте, вас приветсвует ООО 'КИРПИЧ'" FontSize="20" Margin="10,-80,0,0"/>
              <TextBlock Text="Желаете просмотреть доступные данные или авторизоваться?" FontSize="14" Margin="20,-50,0,10"/>
        </StackPanel>
    
        <StackPanel Orientation="Horizontal">              
                        <Button Name="see" Content=" Просмотр данных" Background="SandyBrown" Click="See" Width="150" Height="50" Margin = "30 20 0 10"></Button>
                        <Button Name="autoriz" Content="   Авторизоваться" Background="SandyBrown" Click="Autoriz" Width="150" Height="50" Margin = "140 20 0 10"></Button>
         </StackPanel>     
    </Grid>
</Window>
